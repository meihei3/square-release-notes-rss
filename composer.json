{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "symfony/clock": "^7.2",
        "symfony/console": "7.2.*",
        "symfony/css-selector": "^7.2",
        "symfony/dom-crawler": "^7.2",
        "symfony/dotenv": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "^7.2",
        "symfony/runtime": "7.2.*",
        "symfony/serializer-pack": "^1.3",
        "twig/twig": "^3.18"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.11"
    }
}
